@page "/Courses/{CourseID:int}/Assignments/{ID:int}"
@model A.C.E.S.Pages.Courses.Assignments.AssignmentModel
@{
    ViewData["Title"] = $"{Model.CourseName}: {Model.Assignment.Name}";
}

<form>
    <label>Name <input type="text" /></label>
    <label>Grade <input type="number" /></label>
    <label>Files <input type="file" multiple/></label>
    <label>Unit Tester <input type="file" multiple/></label>
    <input type="submit" value="Add" />
    <input type="button" value="Cancel" />
</form>
